		"dev": "nx serve app",
		"ng": "nx",
		"prepare": "git config core.hookspath .githooks",
		"start": "nx serve app",
		"prepare-cli": "nx g @spartan-ng/tools:hlm-to-cli-generator",
		"lint-publishable": "nx run-many --target=lint --projects=tag:scope:core,tag:scope:brain,tag:scope:helm,tag:scope:cli",
		"lint-fix-publishable": "nx run-many --target=lint --projects=tag:scope:core,tag:scope:brain,tag:scope:helm,tag:scope:cli --fix",
