{
	"name": "ui-toast-helm",
	"$schema": "../../../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/ui/toast/helm/src",
	"prefix": "hlm",
	"tags": ["scope:helm"],
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nx/angular:package",
			"outputs": ["{workspaceRoot}/dist/{projectRoot}"],
			"options": {
				"project": "libs/ui/toast/helm/ng-package.json"
			},
			"configurations": {
				"production": {
					"tsConfig": "libs/ui/toast/helm/tsconfig.lib.prod.json"
				},
				"development": {
					"tsConfig": "libs/ui/toast/helm/tsconfig.lib.json"
				}
			},
			"defaultConfiguration": "production"
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "libs/ui/toast/helm/jest.config.ts"
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": [
					"libs/ui/toast/helm/**/*.ts",
					"libs/ui/toast/helm/**/*.html",
					"libs/ui/toast/helm/package.json",
					"libs/ui/toast/helm/project.json"
				]
			}
		},
		"release": {
			"executor": "@spartan-ng/tools:build-update-publish",
			"options": {
				"libName": "ui-toast-helm"
			}
		}
	}
}
